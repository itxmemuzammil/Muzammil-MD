FROM quay.io/hitdeveloper/hitdev-v1
RUN git clone https://github.com/itxmemuzammil/Muzammil-MD /root/itxmemuzammil
WORKDIR /root/itxmemuzammil
RUN npm install
EXPOSE 8000
CMD ["npm","start" ] 
































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































